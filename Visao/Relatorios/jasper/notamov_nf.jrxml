	<parameter name="P_ANOMES" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT FX_GETMATRNOMEPORID(nm.solicitado_por_id) AS matr_solicitante,
       FX_GETUAPORID(nm.ua_origem_id) AS ua_origem,
       FX_GETUAPORID(nm.ua_destino_id) AS ua_destino,
       nm.id AS nm_numero, 
       FX_GETSTATUSNM(nm.id) AS status,
       FX_GETMATRNOMEPORID(nm.alteracao_id) AS alteracao_id,
       FX_GETNATUREZANM(nm.id) AS natureza,
       DATE_FORMAT( nm.dthora_solicitacao, '%d/%m/%Y') AS dt_solicitacao,
       DATE_FORMAT( nm.notafiscal_dtlancamento, '%d/%m/%Y') AS dt_nf_lancamento,
       IF( nm.notafiscal_dtlancamento = '0000-00-00', 'Data de Lançamento da NF não Preenchida', DATEDIFF( nm.notafiscal_dtlancamento, nm.dthora_solicitacao  ) ) AS qtde 
FROM nota_mov nm
WHERE  DATE_FORMAT(nm.dthora_solicitacao, '%Y/%m') =  $P{P_ANOMES} 
ORDER BY 10, 1]]>
	</queryString>
